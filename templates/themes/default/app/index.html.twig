{% extends 'themes/default/app/layout/layout.html.twig' %}

{% block meta_description %}
    Retrouvez des annonces immobilière autour de Marseille sur Une place au soleil.
    Trouvez votre futur appartement ou une maison en location ou en vente dans la région des Bouches-du-Rhônes. 
    Recherchez facilement un appartement, maison, parking dans notre liste d'annonces immobilières de Une place au soleil.
{% endblock %}
{% block meta_keywords %}uneplaceausoleil, location, vente, location appartement, vente appartement, vente maison, annonces immobiliere, une place au soleil, marseille{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('default_homepage') }}
{% endblock %}

{% block title %}Une place au soleil | Annonces immobilière à Marseille, location d'appartements et vente de maison{% endblock %}

{% block main_content %}
    Hello world 

    <div id="r-compteur" class="compteur" data-count="1946">0</div>

    <ul>
        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
            <li><a href="{{ path('app_logout') }}">Logout</a></li>
            {% if is_granted('ROLE_PREVIOUS_ADMIN') %}
                <li><a href="{{ path('super_users_index', {'_switch_user': '_exit'}) }}">Exit Impersonate</a></li>
            {% endif %}
        {% else %}
            <li><a href="{{ path('app_login') }}">Login</a></li>
        {% endif %}
        <li><a href="{{ path('app_mentions') }}">Mentions</a></li>
        <li><a href="{{ path('app_politique') }}">Politique</a></li>
        <li><a href="{{ path('app_cookies') }}">Cookies</a></li>
        <li><a href="{{ path('app_rgpd') }}">RGPD</a></li>
        <li><a href="{{ path('app_contact') }}">Contact</a></li>
        
    </ul>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('default_homepage') }}
{% endblock %}